# ls
alias ls='ls --color=auto'
alias la='ls -A'
alias lh='ls -Alh'

alias vim='nvim'

alias grep='grep --color=auto'

alias rmtrash='rm -rf ~/.local/share/Trash/*'

alias pa='ps auxf'

#alias chat='ollama serve & ollama run dolphin-llama3'

alias chat='pkill ollama && ollama serve & disown; sleep 3; ollama run dolphin-llama3'

